<table border="1">
  <tr>
    <th>source</th>
    <th>tid</th>
    <th>published</th>
    <th>updated</th>
    <th>read</th>
    <th>clicked</th>
    <th>nfavorite</th>
    <th>nretweet</th>
    <th>score</th>
    <th>text</th>
  </tr>
<% @posts.each do |post| %>
  <tr>
    <td><%= post.source %></td>
    <td><%= post.tid %></td>
    <td><%= post.published %></td>
    <td><%= post.updated %></td>
    <td><%= post.read %></td>
    <td><%= post.clicked %></td>
    <td><%= post.nfavorite %></td>
    <td><%= post.nretweet %></td>
    <td><%= post.score %></td>
    <td><%= post.text %></td>
  </tr>
<% end %>
</table>
